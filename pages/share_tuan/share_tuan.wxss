/**index.wxss**/

.container {
  position: fixed;
  height: 100%;
  width: 100%;
}

.uphalf {
  background-color: #e53517;
  padding-top: 30rpx;
  height: 50%;
  width: 100%;
}

.content {
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  height: 292rpx;
  border-radius: 8rpx;
  display: flex;
  flex-direction: row;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.content_text {
  margin-left: 30rpx;
  margin-right: 30rpx;
  display: flex;
  flex-direction: row;
  margin-top: 10rpx;
  align-items: center;
  font-weight: 500;
}

.middle{
  margin-left: 30rpx;
  margin-right: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-weight: 500;
}

.image {
  height: 220rpx;
  width: 220rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-top: 25rpx;
}

.good_content {
   margin-top: 25rpx;
  height: 220rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.good_text {
  font-size: 28rpx;
  line-height: 36rpx;
  color: #333;
}

.buy_num {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #333;
  margin-right: 10rpx;
}

.yuan {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #e53517;
}

.money {
  font-size: 36rpx;
  line-height: 36rpx;
  color: #e53517;
  font-weight: 600;
}

.button_buy {
  margin: 0rpx;
  font-size: 28rpx;
  height: 60rpx;
  color: #e53517;
  background-color: #fff;
  line-height: 60rpx;
  border: 2rpx solid #e53517;
  margin-top: 20rpx;
  width: 70%;
}

.middle_text {
  width: 100%;
  color: #333;
  font-size: 32rpx;
  text-align: center;
}

.users_content {
  height: 370rpx;
  margin-top: 30rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
  border-radius: 8rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  box-shadow: 0 0 10px rgba(209, 214, 214, 0.5);
}

.image_rows {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}

.image_view {
  height: 70rpx;
  width: 70rpx;
  position: relative;
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.user_image {
  height: 70rpx;
  width: 70rpx;
  border-radius: 50%;
}

.text_tuanzhang {
  position: absolute;
  font-size: 18rpx;
  background-color: #e53517;
  height: 22rpx;
  width: 52rpx;
  bottom: 0rpx;
  color: #fff;
  text-align: center;
  line-height: 22rpx;
  left: 9rpx;
}

.time_rows {
  margin-bottom: 20rpx;
}

.pingdan_text {
  font-size: 32rpx;
  line-height: 36rpx;
  color: #333;
  margin-right: 16rpx;
}

.time_text {
  font-size: 32rpx;
  line-height: 36rpx;
  color: #e53517;
  font-weight: 600;
}

.button_vid {
  background-color: #e53517;
  color: #fff;
  width: 80%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  margin: 0 10rpx;
}

.content_end {
  margin-top: 18rpx;
}

.text_end {
  font-size: 24rpx;
  color: #999;
}

.images {
  height: 70rpx;
  width: 100%;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.image_rows_big {
  position: relative;
  width: 80%;
}

.image_view_big {
  position: absolute;
}

.title_cul {
  display: flex;
  flex-direction: column;
}
.footer_botton{
  width: 100%;
  height: 100rpx;
  position: fixed;
  background: #fff;
  bottom: 0;
  z-index: 96;
  box-shadow: 0px -2rpx 2rpx #f2f2f2;
}
.footer_cart_botton{
  width: 13.333%;
  height: 100rpx;
  float: left;
  border-right: 1px solid #f2f2f2;
  box-sizing:border-box;
}
.footer_cart_botton_contact{
  width: 13.333%;
  height: 100rpx;
  display: inline-block;
  border-right: 1px solid #f2f2f2;
  box-sizing:border-box;
  position:static;
  margin-left:auto;
  margin-right:auto;
  padding-left:0;
  padding-right:0;
  text-align:center;
  text-decoration:none;
  border-radius:0px;
  -webkit-tap-highlight-color:transparent;
  overflow:hidden;
  color:#000000;
  background-color:#fff;
}
.footer_botton button::after{
  border: none;
  width: 0;
  height: 0;
}

.footer_cart_botton image,.footer_cart_botton_contact image{
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin: 0 auto;
  margin-top: 16rpx;
}
.footer_cart_botton text{
  font-size: 20rpx;
  color: #666;
  display: block;
  text-align: center;
  margin-top: 4rpx;
  line-height: 30rpx;
}

.footer_cart_botton_contact text
{
  font-size: 20rpx;
  color: #666;
  display: block;
  text-align: center;
  line-height: 30rpx;
  margin-top: 4rpx;
}