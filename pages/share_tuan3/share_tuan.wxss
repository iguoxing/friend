/* pages/share_tuan/share_tuan.wxss */
page{
  background: #f2f2f2;
}
.tips{
  background-color: #FFF;
  height: 120rpx;
  line-height: 120rpx;
  overflow: hidden;
  padding: 0 100rpx 0 80rpx;
  position: relative;
  margin-bottom: 20rpx;
  font-size: 32rpx;
}
.tips_succ{
  display: block;
  height: 48rpx;
  width: 48rpx;
  background-repeat: no-repeat;
  background-position: center center;
  position: absolute;
  left: 20rpx;
  top: 36rpx;
  border-radius: 24rpx;
  background-color: #fd537b;
  background-image: url('https://lht.she985.com/themes/haohai2017/images/duihao.png');
  background-size: 60%;
}
.tips_err {
  background-image: url('https://lht.she985.com/themes/haohai2017/images/mod_searchpanel_clear.png');
  background-size: cover;
}
.tips_share_button{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 20rpx;
  top: 30rpx;
  font-size: 24rpx;
  color: #fd537b;
  text-decoration: none;
  outline: none;
  text-align: center;
  background-image: url('https://lht.she985.com/themes/haohai2017/images/share_link.png');
  background-size: 100%;
  padding:0;
  background-color:#fff;
}
.tips button::after{
  border: none;
}
.tm{
  background-color: #FFF;
  margin-bottom: 20rpx;
  padding: 20rpx;
  position: relative;
}
.tuanDetailWrap{
  padding-left: 180rpx;
  position: relative;
}
.td_img {
  left: 0;
  overflow: hidden;
  position: absolute;
  top: 0;
  width: 180rpx;
}
.td_img image {
  display: block;
  height: 160rpx;
  overflow: hidden;
  width: 160rpx;
}
.td_info {
  font-size: 24rpx;
}
.td_name {
  color: #333;
  font-size: 28rpx;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  height: 80rpx;
  overflow: hidden;
}
.td_mprice {
  line-height: 60rpx;
  font-size: 28rpx;
}
.td_mprice text {
  padding-right: 40rpx;
}
.tremble {
  animation: tremble 1s linear infinite;
}
@keyframes tremble{
	0%   {transform: rotate(10deg);}
	50%  {transform: rotate(-10deg);}
	100% {transform: rotate(10deg);}
}
.explain_tuan {
  position: absolute;
  bottom: -40rpx;
  right: 20rpx;
  width: 80rpx;
  text-decoration: none;
  outline: none;
  height: 112rpx;
  display: inline;
  z-index: 10;
  background: url('https://lht.she985.com/themes/haohai2017/images/tuan_tip.png') no-repeat;
  background-size: contain;
}

.share_tuan_img{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background: #fff;
  width: 100%;
  height: 100%;
  overflow-y: scroll;
  text-align: right;
}
.share_tuan_img image{
  max-width: 100%;
  border: 0 none;
  width:100%;
  height:100%;
  vertical-align: top;
}

.pp{
  width: 100%;
  font-size: 24rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.pp_users {
  overflow: hidden;
  padding: 20rpx 0;
  text-align: center;
  background: #fff;
}

.pp_users_item {
  display: inline-block;
  height: 98rpx;
  margin-right: 20rpx;
  overflow: hidden;
  width: 98rpx;
}
.pp_users_item image {
  border-radius: 50%;
  display: block;
  height: 92rpx;
  overflow: hidden;
  width: 92rpx;
  background: #ddd;
  padding: 2rpx;
}
.pp_users_normal{
  height: 16px;
  position: absolute;
  right: 0;
  top: 0;
  width: 17px;
  background-image: url('https://lht.she985.com/themes/haohai2017/images/tz.png');
  background-size: cover;
}

.pp_box{
  padding-bottom: 40rpx;
  background: #fff;
}
.pp_tips {
  font-size: 28rpx;
  padding: 20rpx 0;
  text-align: center;
  background: #fff;
}
.pp_tips text {
    color: #fd537b;
    font-size: 36rpx;
    margin: 0 10rpx;
}
.pp_state {
    height: 24px;
    position: relative;
    text-align: center;
}
.pp_time {
    background-color: #fff;
    display: inline-block;
    height: 24px;
    line-height: 24px;
    padding: 0 10px;
    position: relative;
    z-index: 1;
}
.pp_time .time_text{
  background-color:#fd537b;
  border-radius:4rpx;
  color:#FFF;
  display:inline-block;
  height:40rpx;
  line-height:40rpx;
  margin:0 4rpx;
  text-align:center;
  width:40rpx;
  font-size: 30rpx;
}
.pp_state_fgx{
  background-color: #D0D0D0;
  bottom: 22rpx;
  content: "";
  display: block;
  height: 2rpx;
  left: 0;
  overflow: hidden;
  position: absolute;
  width: 100%;
}
.pp_list{
  width: 100%;
  overflow: hidden;
  padding-top: 30rpx;
  line-height: 30rpx;
  font-size: 30rpx;
  position: relative;
  margin-top: -30rpx;
}
.pp_list_item{
  background-color: #FFF;
  margin-bottom: 20rpx;
  padding: 20rpx 30rpx 20rpx 100rpx;
  position: relative;
}
.pp_list_item:nth-child(1) {
    background-color: #f2f2f2;
    border-color: #f2f2f2;
    color: #333;
    margin-bottom: 0;
}
.shanjiao{
    border-color: transparent transparent #f2f2f2;
    border-style: dashed dashed solid;
    border-width: 16rpx;
    content: "";
    display: inline-block;
    font-size: 0;
    height: 0;
    left: 50%;
    margin-left: -16rpx;
    overflow: hidden;
    position: absolute;
    top: -32rpx;
    width: 0;
}
.pp_list_avatar {
    background-clip: padding-box;
    border-radius: 60rpx;
    display: block;
    height: 60rpx;
    left: 20rpx;
    position: absolute;
    top: 16rpx;
    width: 60rpx;
    vertical-align: middle;
}
.pp_list_info{
    height: 60rpx;
    line-height: 60rpx;
}
.pp_list_name {
    float: left;
}
.pp_list_time {
    float: right;
}
.pp_list_blank {
    color: #333;
    height: 100rpx;
    line-height: 100rpx;
    margin-bottom: 20rpx;
    padding-left: 100rpx;
    position: relative;
    background: #fff;
}
.pp_time_text{
    display: inline-block;
}
.chamemeber_touxiang{
  background-image: url('https://lht.she985.com/themes/haohai2017/images/bg.png');
  background-repeat: no-repeat;
  background-size: 365px 490px;
  background-position: -324px -41px;
  content: "";
  display: block;
  height: 68rpx;
  left: 16rpx;
  position: absolute;
  top: 16rpx;
  width: 68rpx;
  box-sizing: border-box;
}
.step{
  background-color: #FFF;
  font-size: 24rpx;
  padding: 30rpx 20rpx;
  position: relative;
}
.step_hd {
    height: 36rpx;
    line-height: 36rpx;
    margin-bottom: 24rpx;
    color: #fd537b;
    font-size: 24rpx;
}

.step_more {
  color: #333;
  display: inline-block;
  height: 36rpx;
  line-height: 36rpx;
  text-align: right;
  float: right;
  margin-right: 20rpx;
  font-size: 24rpx;
}
.right_icon{
  background-image: url('https://lht.she985.com/themes/haohai2017/images/bg.png');
  background-repeat: no-repeat;
  background-size: 365px 490px;
  background-position: -303px -102px;
  display: inline-block;
  height: 24rpx;
  margin-left: 10rpx;
  float: right;
  width: 12rpx;
  margin-top: 8rpx;
}
.step_more navigator{
  display: inline-block;
  float: left;
  vertical-align: middle;
}
.step_list{
  display: block;
  width: 100%;
  height: 40rpx;
  background: #fff;
}
.step_item {
  float: left;
  position: relative;
  width: 25%;
}
.step_num {
    background-clip: padding-box;
    background-color: #FFF;
    border: 1px dashed #D5D5D5;
    border-radius: 40rpx;
    color: #666;
    height: 40rpx;
    left: 0;
    line-height: 40rpx;
    position: absolute;
    text-align: center;
    top:4rpx;
    width: 40rpx;
}
.step_detail {
  padding-left: 50rpx;
}
.step_tit {
  color: #666;
  font-size: 20rpx;
  line-height: 24rpx;
}
.step_tit text{
  display: block;
}
.step_item_on .step_num {
    background-color: #fd537b;
    border: 1px solid #fd537b;
    color: #FFF;
}
.step_item_on .step_tit {
    color: #fd537b;
}
.td_mprice_text{
  color: #e95a7a;
  font-size: 30rpx;
}

/*底部  */
.fixopt{
  background-color: rgba(0,0,0,.6);
  bottom: 0;
  left: 0;
  padding: 20rpx 0;
  position: fixed;
  width: 100%;
  z-index: 901;
}
.fixopt_item2 {
    padding-left: 52px;
}
.fixopt_item {
    min-height: 32px;
    position: relative;
}
.fixopt_home {
  background-color: #777;
  border-radius: 3px;
  display: block;
  height: 42px;
  left: 10px;
  position: absolute;
  top: 0;
  width: 42px;
  color: #fff;
}
.fixopt_home_icon{
  background-position: -305px -277px;
  content: "";
  display: block;
  height: 48rpx;
  left: 18rpx;
  position: absolute;
  top: 18rpx;
  width: 48rpx;
  background-image: url('https://lht.she985.com/themes/haohai2017/images/bg.png');
  background-repeat: no-repeat;
  background-size: 365px 490px;
}
.fixopt_btn {
  background-color: #fd537b;
  border-radius: 6rpx;
  color: #FFF;
  display: block;
  height: 84rpx;
  line-height: 84rpx;
  font-size: 30rpx;
  margin: 0 20rpx;
  text-align: center;
}
.fixopt_zhanwei{
  height: 160rpx;
  width: 100%;
  display: block;
}
