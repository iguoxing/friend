/*设置数量-加入购物车弹出窗  */
.speDiv{
  position: fixed;
  bottom: 100rpx;
  z-index: 99;
  background: #fff;
  width: 100%;
}
.sku-head{
  padding: 20rpx 20rpx 0 20rpx;
  position: relative;
  height: 160rpx;
}
.skuimage{
  height: 180rpx;
  width: 180rpx;
  padding: 6rpx;
  border: 1px solid #d3d3d3;
  position: absolute;
  left: 20rpx;
  top: -30rpx;
  border-radius: 10rpx;
  background: #fff;
}
.sku-detail{
  display: inline-block;
  margin-left: 210rpx;
  margin-right: 10rpx;
  height: 140rpx;
}
.sku-name{
  font-size: 24rpx;
  position: relative;
  color: #000;
  line-height: 36rpx;
  max-height: 72rpx;
  overflow: hidden;
}
.sku-price2-depends{
  color: #fd537b;
  font-size: 28rpx;
  line-height: 36rpx;
}
.attr-stock{
  float: left;
  font-size: 24rpx;
  color: #fd537b;
}
/*数量修改input  */
.sku-amount{
  height: 100rpx;
  clear: both;
  width: 100%;
  padding: 18rpx 0;
  display: block;
  background-color: #fff;
  box-sizing: border-box;
  border-top: 1px solid #dedede;
}
.sku-text text{
  font-size: 28rpx;
  color: #000;
  line-height: 60rpx;
  height: 60rpx;
  text-align: center;
  display: inline-block;
  float: left;
  padding-left: 28rpx;
}
.nbox{
  width: 200rpx;
  height: 60rpx;
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  background: #eaeaea;
  border-radius: 30rpx;
  overflow: hidden;
}
.fa{
  display: inline-block;
  font: normal normal normal 14px/1 FontAwesome;
  font-size: 30rpx;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  transform: translate(0, 0);
  font-weight: bold;
}
.fa-minus{
    background: #fff;
    color: #fd537b;
}
.hui{
  color: #eee;
}
.nbox .num {
  float: left;
  width: 80rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #fd537b;
  text-align: center;
  border: none;
  background: none;
}
.nbox .fa-plus {
  background: #fd537b;
  color: #fff;
}
.nbox view{
  float: left;
  width: 60rpx;
  height: 60rpx;
  line-height: 55rpx;
  border-radius: 30rpx;
  text-align: center;
}
.sku-quit{
  cursor: pointer;
  width: 60rpx;
  height: 48rpx;
  position: absolute;
  top: -48rpx;
  right: 10rpx;
  background: url('https://lht.she985.com/xcx/images/sku-quit.png');
  background-size: 100%;
}

