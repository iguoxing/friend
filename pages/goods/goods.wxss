/*返回订单按钮  */
.back-top{
  width: 42px;
  height: 42px;
  position: fixed;
  right: 20px;
  bottom: 64px;
  z-index: 97;
  background: url('https://lht.she985.com/themes/haohai2017/images/go_top.png') no-repeat;
  background-size: contain;
  -webkit-transition: bottom .8s ease,opacity .6s ease;
}
.back-top text{
  position: absolute;
  bottom: 7px;
  width: 100%;
  display: block;
  height: 12px;
  line-height: 12px;
  text-align: center;
  font-size: 10px;
  color: #333;
}

