}

.cart_body{
  background: #f5f5f5;
}

.supplier_text{
  font-size: 11pt;
  float: left;
  line-height: 80rpx;
  color: #000000;
  font-weight: bold;
}

.cart_null image{
  height: 300rpx;
  width: 300rpx;
  display: block;
  margin: 20rpx auto;
}
.cart_null text{
  margin-top: 50rpx;
