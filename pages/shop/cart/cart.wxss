  display: inline-block;
  line-height: 23rpx;
  height: 46rpx;
}
.goods_price{
  font-size: 25rpx;
  margin-left: 10rpx;
  color: #b2b2b2;
  display: inline-block;
  line-height: 23rpx;
  height: 46rpx;
  color: red;
}
.num{
  display: block;
  clear: both;
