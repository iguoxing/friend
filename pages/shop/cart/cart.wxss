  background-size: 60px;
  border-radius:0;
  text-align: center;
  line-height: 50rpx;
  margin: 0rpx 0 0 10rpx;
  float: left;
  border: 1px solid #e0e0e0;
}


.num_input{
  float: left;
  width: 52rpx;
  height: 52rpx;
  margin: 0rpx 0 0 0;
  border-bottom: 1px solid #e0e0e0;
  text-align: center;
  line-height: 25px;
  border-top: 1px solid #e0e0e0;
  
}

.num_button2{
  height: 52rpx;
  font-size: 12pt;
  width: 52rpx;
  background: url('http://www.hengda365.com/xcx/images/shop-cart.png') no-repeat -26px -27px;
  background-size: 60px;
  border-radius:0;
  text-align: center;
  line-height: 50rpx;
  margin: 0rpx 0 0 0;
  float: left;
  border: 1px solid #e0e0e0;
}
