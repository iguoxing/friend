}
.cart_checkbox{
  width: 50rpx;
  height: 50rpx;
  z-index: 9;
  vertical-align:middle;
  display: inline-block;
  margin-left:20rpx;
  margin-right: 10rpx;
  background: url('http://www.hengda365.com/xcx/images/shop-cart.png') no-repeat 0 0;
  background-size: 60px 120px;
  float: left;
  margin-top: 90rpx;
}
.checked{
  background-position: -30px 0;
}
.quanxuan{
  margin-top: 25rpx;
}

.quanxuan_text{
  float: left;
  line-height: 100rpx;
  font-size: 30rpx;
}

.cart_list_image{
    width: 136rpx;
    height: 136rpx;
    margin-top: 30rpx;
    border: #e0e0e0 1px solid;
    display: inline-block;
    float: left;
}
.goods_list{
  height: 230rpx;
