/* pages/user_login/user_login.wxss */
page{
  background: #e1313a;
}
.user_login{
  width: 100%;
}
.user_login_button{
  width: 400rpx;
  height: 122rpx;
  display: block;
  text-align: center;
  margin: 0 auto;
}
.user_logn_top{
  width: 100%;
  display: block;
  margin: 0 auto;
}


.user_contact_button{
  position:static;
  display:block;
  margin-left:auto;
  margin-right:auto;
  padding-left:0;
  padding-right:0;
  font-size:30rpx;
  text-align:center;
  text-decoration:none;
  line-height:40rpx;
  border-radius:0px;
  -webkit-tap-highlight-color:transparent;
  overflow:hidden;
  border: none;
  color:#000000;
  background: #e1313a;
}
.fff{
  background: #e1313a;
}
.fff button::after{
  border: none;
  width: 0;
  height: 0;
  background: red;
}