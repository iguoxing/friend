
page{
  background: #f2f2f2;
}
/*顶部菜单  */
.cat_list{
  height: 80rpx;
  display: block;
  margin-bottom: 10rpx;
  border-bottom: 1px solid #f3f3f3;
  border-top: 1px solid #e5e5e5;
  background: #fff;
}
.cat_list-navbar{
  height: 80rpx;
}

.cat_list_item{
  display: inline-block;
  margin-left: 20rpx;
  overflow:hidden;
  height: 76rpx;
  text-align: center;
  font-size: 26rpx;
  line-height: 76rpx;
  float: left;
  width: 112rpx;
  color: #333;
}
.cat_list_item_on{
  border-bottom: 2px #e95a7a solid;
}

/*团购商品列表  */
.tuan_list {
  overflow: hidden;
  width: 98%;
  margin: 0 auto;
}
.tuan_g{
  margin-bottom: 16rpx;
  display: block;
  background: #fff;
}
.tuan_g_img image{
  overflow: hidden;
  width: 100%;
  min-height: 360rpx;
}
.tuan_g_info{
  padding-top: 10px;
}
.tuan_g_name{
  margin: 0 10rpx 10rpx 10rpx;
  font-size: 28rpx;
  color: #555;
  line-height: 1.4;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-wrap: break-word;
  word-break: break-all;
}

.tuan_g_core {
  display: block;
  padding: 10px 0;
  position: relative;
  overflow: hidden;
  color: #fd537b;
}
.tuan_g_core_image{
  float: left;
  margin-left: 10rpx;
  width: 60rpx;
  height: 60rpx;
  background-size: 100%;
}

.tuan_g_num{
  float: left;
  padding-left: 2%;
  line-height: 30px;
  font-size: 16px;
}
.tuan_fgx{
  width: 1px;
  float: left;
  height: 20px;
  margin-top: 5px;
  margin-left: 2%;
  border-left: #eee 1px solid;
  overflow: hidden;
}
.tuan_g_price{
  float: left;
  height: 100%;
  padding-left: 2%;
  line-height: 30px;
  color: #fd537b;
  font-size: 18px;
}
.tuan_g_mprice {
  display: inline-block;
  color: #999;
  line-height: 30px;
  font-size: 12px;
  text-decoration: line-through;
  padding-left: 5px;
}
.tuan_g_price_fuhao{
  font-size: 28rpx;
}
.tuan_g_btn{
  float: right;
  margin-right: 10px;
  width: 80px;
  height: 30px;
  background-color: #fd537b;
  border-radius: 5px;
  line-height: 30px;
  text-align: center;
  font-size: 14px;
  color: #fff;
  display: inline-block;
}


/*商品列表2  */
.goods2_one{
  width: 96%;
  display: block;
  height: 250rpx;
  box-shadow: 0 0 10rpx #ccc;
  margin: 10rpx auto;
  border:1px solid #f2f2f2;
  margin-bottom: 20rpx;
  background: #fff;
}
.goods2_one_img{
  height: 220rpx;
  width: 220rpx;
  display: block;
  margin-left: 10rpx;
  padding-top: 15rpx;
  float: left;
}
.goods2_one_right{
  display: block;
  float: left;
  margin-top: 15rpx;
  height: 220rpx;
  width: 64%;
  margin-left: 26rpx;
}
.goods2_one_name{
  font-size: 26rpx;
  line-height: 30rpx;
  height: 60rpx;
  display: block;
  overflow: hidden;
  width: 100%;
}
.goods2_one_name2{
  font-size: 26rpx;
  line-height: 30rpx;
  height: 60rpx;
  display: block;
  overflow: hidden;
  width: 100%;
  margin-top: 10rpx;
  color: #b2b2b2;
}
.tuan_g_core2{
  height: 56rpx;
  line-height: 56rpx;
  margin-top: 40rpx;
  background-color: transparent;
}
.tuan_g_num2{
  height: 56rpx;
  line-height: 56rpx;
  font-size: 12px;
  float: left;
  color: #fd537b;
}
.line2{
  height: 36rpx;
  width: 2rpx;
  float: left;
  margin-top: 10rpx;
  margin-left: 2%;
  border-left: #eee 1px solid;
  overflow: hidden;
}
.tuan_g_price2{
  height: 56rpx;
  line-height: 56rpx;
  font-size: 24rpx;
  color: #fd537b;
  float: left;
}
.tuan_g_mprice2{
  line-height: 60rpx;
  height: 56rpx;
  float: left;
  display: inline-block;
  color: #999;
  font-size: 24rpx;
  text-decoration: line-through;
  padding-left: 10rpx;
}
.tuan_g_btn2{
  width: 100rpx;
  height: 56rpx;
  background-color: #fd537b;
  border-top-left-radius: 46rpx;
  border-bottom-left-radius: 40rpx;
  line-height: 56rpx;
  font-weight: bold;
  text-align: center;
  font-size: 26rpx;
  color: #fff;
  display: block;
  float: right;
  margin-right: 20rpx;
}
/*头部-搜索框  */
.page_header{
  height: 120rpx;
  width: 100%;
  z-index: 9;
