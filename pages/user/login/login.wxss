/*login.wxss*/

.body{
  margin: 100rpx;

}

.tis{
  width: 100%;
  background: #fff;
  color: #fff;
  text-align: center;
  line-height: 30px;
  height: 60rpx;
  font-size: 30rpx;
}

.header{
  border-bottom: 2px solid #09BB07;
  margin:20rpx 0;
  display: block;
  text-align: center;
}

.login_from{
  margin-top: 40rpx;
}

.login_input{
  border-bottom: 1px solid #353535;
  height: 80rpx;
}
.login_from text{
  font-size: 30rpx;
}
.login_div{
  margin-top: 30rpx;
}

.login_button{
  margin-top: 30rpx;
}
.login_additional{
  font-size: 30rpx;
  margin-top: 50rpx;
}
.additional_nav{
  display: inline-block;
  color: blue;
  margin: 0;
}

.additional_nav2{
  display: inline-block;
  color: blue;
  margin: 0;
  float: right;
}

.quick_login{
  margin-top: 100rpx;
  width: 100%;
  height: 200rpx;
}

.quick_login_text{
  font-size: 30rpx;
  text-align: center;
  display: block;
  margin-bottom:20rpx;
}
.quick_icon{
  display: block;
  margin: 0 auto;
  text-align: center;
}
.quick_icon image{
  width: 96rpx;
  height: 96rpx;
}